{
	"schemaVersion": 1,
	"id": "prefixpals",
	"version": "1.0.0",
	"name": "PrefixPals",
	"description": "A Fabric mod for Minecraft 1.21.11 that lets you add custom prefixes and suffixes to in-game chat messages. You can easily edit and customize these chat pals with simple commands, making your server chat more lively and personalized.",
	"authors": [
		"PickStars!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/PickStars308/PrefixPals"
	},
	"license": "MIT license",
	"icon": "assets/prefixpals/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"top.xinstudio.prefixpals.PrefixPals"
		],
		"fabric-datagen": [
			"top.xinstudio.prefixpals.PrefixPalsDataGenerator"
		]
	},
	"mixins": [
		"prefixpals.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~1.21.11",
		"java": ">=21",
		"fabric-api": "*"
	}
}